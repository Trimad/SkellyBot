{
    "1.1": [
        {
            "Q": "SSH",
            "A": [
                "Secure Shell",
                "TCP Port 22",
                "Encrypted communication link",
                "A secure replacement to Telnet"
            ]
        },
        {
            "Q": "DNS",
            "A": [
                "Domain Name Service/Domain Name Server/Domain Name System",
                "UDP Port 53",
                "Maps domain names to various types of data, such as IP addresses."
            ]
        },
        {
            "Q": "SMTP",
            "A": [
                "Simple Mail Transfer Protocol",
                "TCP Port 25",
                "Server-to-server email transfer",
                "Commonly configured on mobile devices and email clients"
            ]
        },
        {
            "Q": "SFTP",
            "A": [
                "Secure File Transfer Protocol",
                "TCP Port 22",
                "Uses SSH as the underlying protocol to make this happen"
            ]
        },
        {
            "Q": "FTP",
            "A": [
                "File Transfer Protocol",
                "TCP Port 20 for \"active mode\" data transfer",
                "TCP Port 21 for \"control\"",
                "Authenticates with a username and password"
            ]
        },
        {
            "Q": "TFTP",
            "A": [
                "Trivial File Transfer Protocol",
                "UDP Port 69",
				"Provides no security features",
                "No authentication, no encryption",
                "A very basic form of file sharing protocol"
            ]
        },
        {
            "Q": "TELNET",
            "A": [
                "Telecommunication Network",
                "TCP Port 23",
                "Transmits data in an unencrypted form \"in-the-clear communication\"",
                "Provides username and password authentication",
				"Enables remote login and command execution"
            ]
        },
        {
            "Q": "DHCP",
            "A": [
                "Dynamic Host Configuration Protocol",
                "UPD Port 67 and UDP Port 68",
                "Requires a DHCP server on your network",
                "IP addresses are assigned in real-time from a pool of available IP addresses"
            ]
        },
        {
            "Q": "HTTP",
            "A": [
                "Hypertext Transfer Protocol",
                "TCP Port 80",
                "Web server communication \"in the clear\""
            ]
        },
        {
            "Q": "HTTPS",
            "A": [
                "Hypertext Transfer Protocol Secure",
                "TCP Port 443",
                "Secures web traffic via SSL/TLS encryption"
            ]
        },
        {
            "Q": "SNMP",
            "A": [
                "Simple Network Management Protocol",
                "UDP Port 161",
                "Gathers statistics from network attached devices"
            ]
        },
        {
            "Q": "RDP",
            "A": [
                "Remote Desktop Protocol",
                "TCP Port 3389",
                "Can connect to an entire desktop or just an application"
            ]
        },
        {
            "Q": "NTP",
            "A": [
                "Network Time Protocol",
                "UDP Port 123",
                "Synchronizes clocks across all network devices",
                "Very accurate, better than 1 millisecond on a local network"
            ]
        },
        {
            "Q": "SIP",
            "A": [
                "Session Initiation Protocol",
                "TCP Port 5060 and TCP 5061",
                "Call, ring and hang up for VoIP",
                "Supports video conferencing, instant messaging and file transfer"
            ]
        },
        {
            "Q": "SMB/CIFS",
            "A": [
                "Server Message Block/Common Internet File System",
                "TCP Port 445",
				"Provides shared access to files, directories, and devices",
                "Used mainly by computers running Microsoft Windows operating systems"
            ]
        },
        {
            "Q": "POP3",
            "A": [
                "Post Office Protocol 3",
                "TCP Port 110",
                "Basic mail transfer functionality",
                "Email retrieval"
            ]
        },
        {
            "Q": "IMAP4",
            "A": [
                "Internet Message Access Protocol 4",
                "TCP Port 143",
                "Serves the same purpose of POP but provides improved functionality",
                "Includes management of email inbox from multiple devices.",
                "Typically used more often than POP3"
            ]
        },
        {
            "Q": "LDAP",
            "A": [
                "Lightweight Directory Access Protocol",
                "TCP Port 389",
                "Store and retrieve information in a network directory"
            ]
        },
        {
            "Q": "LDAPS",
            "A": [
                "Lightweight Directory Access Protocol Secure",
                "TCP Port 636",
                "SSL/TLS-based directory access protocol"
            ]
        },
        {
            "Q": "H.323",
            "A": [
                "VoIP signaling",
                "TCP Port 1720",
                "setup and manage VoIP sessions; call, ring, hang up"
            ]
        },
        {
            "Q": "ICMP",
            "A": [
                "Internet Control Message Protocol",
                "A protocol used by routers, hosts and network devices to generate error messages and troubleshoot problems with delivery of IP packets."
            ]
        },
        {
            "Q": "UDP",
            "A": [
                "User Datagram Protocol",
                "UDP is an example of a connectionless protocol. Because UDP doesn't support three-way handshake while establishing a network connection, it is referred to as unreliable or best-effort protocol."
            ]
        },
        {
            "Q": "TCP",
            "A": [
                "Transmission Control Protocol",
                "TCP is an example of a connection-oriented protocol using a three-way handshake which is a set of initial steps required for establishing network connection. TCP supports retransmission of lost packets, flow control (managing the amount of data that is being sent), and sequencing (rearranging packets that arrived out of order)."
            ]
        },
        {
            "Q": "IP",
            "A": [
                "Internet Protocol",
                "A network layer protocol that specifies the format of packets and addressing scheme in network communications."
            ]
        },
        {
            "Q": "\"Connection-oriented vs. connectionless\"",
            "A": [
                "UDP vs TCP"
            ]
        }
    ],
    "1.2": [
        {
            "Q": "OSI Model Layers",
            "A": [
                "7. Application Layer",
                "6. Presentation Layer",
                "5. Session Layer",
                "4. Transport Layer",
                "3. Network Layer",
                "2. Data Link Layer",
                "1. Physical Layer"
            ]
        },
        {
            "Q": "OSI model mnemonic device",
            "A": [
                "All",
                "People",
                "Seem",
                "To",
                "Need",
                "Data",
                "Processing"
            ]
        },
        {
            "Q": "OSI Layer 1",
            "A": [
                "Physical Layer",
                "The signal that transports our traffic across the network",
                "This layer isn't about protocols",
                "A physical layer problem may entail fixing cabling, punch-downs, etc."
            ]
        },
        {
            "Q": "OSI Layer 2",
            "A": [
                "Data Link Layer",
                "Is functionally the \"switching\" layer",
                "Frames, MAC addresses, Switches, Bridges, etc."
            ]
        },
        {
            "Q": "OSI Layer 3",
            "A": [
                "Network Layer",
                "Is functionally the \"routing\" layer",
                "IP addresses, routers, packets, etc.",
                "Frames are fragmented at the network layer"
            ]
        },
        {
            "Q": "OSI Layer 4",
            "A": [
                "Transport Layer",
                "Is functionally the \"post office\" layer",
                "UDP",
                "TCP"
            ]
        },
        {
            "Q": "OSI Layer 5",
            "A": [
                "Session Layer",
                "Assumes the responsibility for managing network connections between applications.",
                "Start and stop communications"
            ]
        },
        {
            "Q": "OSI Layer 6",
            "A": [
                "Presentation Layer",
                "Character encoding, data compression and encryption/decryption take place."
            ]
        },
        {
            "Q": "OSI Layer 7",
            "A": [
                "Application Layer",
                "This is the layer we see.",
                "HTTP",
                "FTP",
                "SMTP"
            ]
        }
    ],
